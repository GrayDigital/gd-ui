{
  "name": "gd-ui",
  "version": "0.0.0-development",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/GrayDigital/gd-ui"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.15",
    "@chakra-ui/react": "^1.0.0",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@snek-at/storybook-addon-chakra-ui": "^1.0.0-beta.1",
    "@testing-library/react": "^10.2.1",
    "@testing-library/user-event": "^12.0.2",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "framer-motion": "^4.0.0",
    "node-env-run": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "4.1.0",
    "react-scripts": "4.0.3",
    "typescript": "^3.9.5",
    "web-vitals": "^0.2.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "sb": "start-storybook -p 6006 -s ./public",
    "build-sb": "build-storybook -s public",
    "sr": "node-env-run --exec 'semantic-release --ci false'"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.11",
    "@storybook/addon-essentials": "^6.3.11",
    "@storybook/addon-links": "^6.3.11",
    "@storybook/node-logger": "^6.3.11",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.11",
    "semantic-release": "^18.0.0"
  }
}
